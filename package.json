{
  "name": "itp-h5p",
  "version": "1.0.0",
  "description": "H5P Interactive Content Platform",
  "main": "index.js",
  "scripts": {
    "start": "cd backend && node server.js",
    "build": "cd frontend && npm run build",
    "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm start\"",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "railway:build": "npm install && cd frontend && npm install && npm run build && cd ../backend && npm install",
    "railway:start": "cd backend && node server.js",
    "railway:migrate": "cd backend && node scripts/initDb.js",
    "setup-wordpress": "./setup-wordpress.sh",
    "setup-wordpress-mamp": "./setup-wordpress-mamp.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "h5p",
    "education",
    "interactive",
    "content"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/hls.js": "^0.13.3",
    "concurrently": "^8.2.2",
    "react-scripts": "5.0.1"
  },
  "dependencies": {
    "hls.js": "^1.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
